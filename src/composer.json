{
  "require": {
    "php": ">=7.1.16",
    "php-di/php-di": "*",
    "nikic/fast-route": "^1.3",
    "illuminate/database": "^5.6",
    "illuminate/events": "^5.6",
    "twig/twig": "^2.4",
    "robmorgan/phinx": "^0.9.2",
    "illuminate/pagination": "^5.6"
  },
  "require-dev": {
      "phpunit/phpunit": "*"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  }
}
